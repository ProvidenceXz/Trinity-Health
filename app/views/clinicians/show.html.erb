
<h2 class="text-center">Clinician: <strong><%= @clinician.name %></strong></h3>
<div class="well col-xs-8 col-xs-offset-2">
  
  <h4><strong>Email: <%= @clinician.email %></strong></h4>
  <h4><strong>Specialty:</strong></h4>
  <%= simple_format(@clinician.specialty) %>
  <% if @allowed %>
    <div class="clinician-action">
      <%= link_to "Edit Profile", edit_clinician_path, class: "btn btn-xs btn-primary" %>
   </div>
  <% end %>
</div>
